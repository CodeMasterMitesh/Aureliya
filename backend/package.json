{
  "name": "aureliya-backend",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "lint": "eslint \"src/**/*.js\"",
    "format": "prettier --write .",
    "seed": "node src/scripts/seed.js",
    "seed:menus": "node src/scripts/seedMenus.js",
    "db:inspect": "node src/scripts/dbInspect.js",
    "migrate:ledgers-to-users": "node src/scripts/migrateLedgersToUsers.js",
    "import:contacts": "node src/scripts/importContactsToUsers.js",
    "generate:schemas": "node src/scripts/generateSchemasFromSql.js",
    "audit:schemas": "node src/scripts/auditGeneratedSchemas.js",
    "import:companies-branches": "node src/scripts/importCompaniesBranches.js",
    "import:account-groups": "node src/scripts/importAccountGroups.js",
    "import:products": "node src/scripts/importProducts.js",
    "import:transactions": "node src/scripts/importTransactions.js",
    "db:create-collections": "node src/scripts/createCollections.js",
    "db:create-collections:dry": "cross-env DRY_RUN=true node src/scripts/createCollections.js",
    "db:create-collections:indexes": "cross-env BUILD_INDEXES=true node src/scripts/createCollections.js",
    "db:seed-placeholders": "node src/scripts/seedPlaceholders.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "envalid": "^8.0.0",
    "express": "^4.21.1",
    "express-rate-limit": "^7.5.1",
    "express-session": "^1.18.2",
    "express-validator": "^7.0.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.8.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.15",
    "pino": "^9.2.0",
    "pino-http": "^10.0.0",
    "helmet": "^7.1.0",
    "cookie-parser": "^1.4.6",
    "csurf": "^1.11.0",
    "razorpay": "^2.9.5",
    "sweetalert2": "^11.26.3",
    "mysql2": "^3.11.3"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.7",
    "pino-pretty": "^11.2.2",
    "supertest": "^7.0.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.6.0",
    "prettier": "^3.3.3"
  }
}
